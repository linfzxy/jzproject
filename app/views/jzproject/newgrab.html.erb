<%= form_for :jzproject do |f| %>
    <p>
      <%= f.label 'url链接' %><br>
      <%= f.text_area :url %>
    </p>

    <p>
      <%= f.label 'cookie记录' %><br>
      <%= f.text_area :cookie %>
    </p>

    <p>
      <%= f.label '启始页数' %><br>
      <%= f.text_field :startpage, :value => "0"%>
    </p>

    <p>
      <%= f.label '请求间隔/s' %><br>
      <%= f.text_field :gap, :value => "5"%>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>
